{
    "name": "martinschenk/livewire-cookie-consent",
    "description": "Laravel Livewire cookie consent modal for EU",
    "keywords": ["laravel", "livewire", "cookie", "consent", "cookieconsent", "modal"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schenk",
            "email": "mschenk.pda@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^2.5",
        "wire-elements/modal": "^1.0",
        "illuminate/http": "^8.0|^9.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Martinschenk\\LivewireCookieConsent\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Martinschenk\\LivewireCookieConsent\\CookieConsentModalServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
