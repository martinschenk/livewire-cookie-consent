{
    "name": "martinschenk/livewire-cookie-consent",
    "description": "livewire cookie consent modals for EU with laravel livewire",
    "keywords": ["laravel", "livewire", "cookie", "consent", "cookie-consent", "modal", "law", "EU"],
    "homepage": "https://github.com/martinschenk/livewire-cookie-consent",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schenk",
            "email": "mschenk.pda@gmail.com",
            "homepage": "https://www.martin-schenk.es",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "livewire/livewire": "^2.5",
        "wire-elements/modal": "^1.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Martinschenk\\LivewireCookieConsent\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Martinschenk\\LivewireCookieConsent\\CookieConsentModalServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
